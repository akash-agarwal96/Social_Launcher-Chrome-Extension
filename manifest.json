{
    "manifest_version": 2,
  
    "name": "Social Launcher",
    "description": "Quick launch Social handles",
    "version": "1.0.1",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["activeTab"]
  }